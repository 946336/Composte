register rob password mail@mai.com
set x `create-project rob proj {}`
set pid `slice 9 -3 $x`
export pid $pid
get-project $pid
add-instrument $pid 0.0 0 flute
insert-metronome-mark $pid 0.0 114
add-dynamic $pid 0.0 0 mp
insert-note $pid 1.0 0 D5 0.5
insert-note $pid 1.5 0 E5 0.5
insert-note $pid 2.0 0 F5 0.5
insert-note $pid 2.5 0 F5 0.5
insert-note $pid 3.0 0 G5 0.5
insert-note $pid 3.5 0 E5 0.75
insert-note $pid 4.25 0 D5 0.25
insert-note $pid 4.5 0 C5 2.0 
